<?xml version="1.0"?>

<tool id="geec_data_source" name="GeEC Public Datasets" tool_type="data_source">
  <description>description</description>
  <command interpreter="python"> data_source.py $output $__app__.config.output_size_limit</command>

  <inputs action="http://127.0.0.1:8000/" check_values="false" method="get">
    <param name="GALAXY_URL" type="baseurl" value="/tool_runner" />
    <param name="tool_id" type="hidden" value="geec_data_source" />
    <param name="sendToGalaxy" type="hidden" value="1" />
  </inputs>

  <outputs>
    <data format="tabular" name="output" />
  </outputs>

  <options sanitize="False" refresh="True"/>

</tool>
